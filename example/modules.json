{
  "mode": "tolerant",
  "groups": {
    "entities": {
      "dir": "/entity",
      "ignore": ["IgnoredModule.js"],
      "alias": {
        "Dummy": "DummyModule"
      },
      "data": {
        "type": "class",
        "initiate": "multiple"
      }
    },
    "missing-dir": {
      "dir": "/dummy-dir",
      "data": {
        "type": "object"
      }
    },
    "services": {
      "data": {
        "type": "class",
        "initiate": "singleton"
      }
    }
  },
  "modules": {
    "app": {
      "module": "/app",
      "type": "object",
      "injections": {
        "authService": "authService",
        "userService": "userService"
      }
    },
    "db": {
      "module": "/db",
      "type": "object",
      "postConstructor": "init"
    },
    "utils": {
      "module": "/utils"
    },
    "userService": {
      "module": "/service/user",
      "type": "class"
    },
    "authService": {
      "module": "/service/auth",
      "extends": ["services"],
      "injections": {
        "utils": "utils"
      }
    }
  }
}